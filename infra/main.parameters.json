          "existingAccountResourceGroupName": "${STORAGE_RESOURCE_GROUP_NAME}",
          "existingStorageName": "${STORAGE_NAME}",
          "vnetReuse": "${VNET_REUSE}",
          "existingVnetResourceGroupName": "${VNET_RESOURCE_GROUP_NAME}",
          "existingVnetName": "${VNET_NAME}"
